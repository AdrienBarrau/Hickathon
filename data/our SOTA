missing_threshold = 0.8
selector = VarianceThreshold(threshold=0.01)
top_n_features = min(200, len(X_train.columns))
model = xgb.XGBRegressor(
    n_estimators=1000,
    learning_rate=0.03,
    max_depth=12,
    min_child_weight=3,
    subsample=0.7,
    colsample_bytree=0.7,
    gamma=0.5,
    reg_alpha=0.5,
    reg_lambda=2,
    random_state=42,
    n_jobs=-1,
    early_stopping_rounds=50
)

